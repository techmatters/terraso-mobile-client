    "typescript": "~5.9.3"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest.unit.config.js",
      "<rootDir>/jest.integration.config.js"
    ]
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-application",
        "expo-keep-awake"
      ]
    }
  },
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**",
    "src/tests",
    "jest"
  ],
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "sh -c 'npm run eslint && npm run check-ts'"
    ],
    "**/*.{json,ts,tsx,html}": [
      "sh -c 'npm run prettier'"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
